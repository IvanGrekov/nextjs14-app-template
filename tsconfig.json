{
    "compilerOptions": {
        "target": "ES2022",
        "lib": ["dom", "dom.iterable", "esnext"],
        "noImplicitAny": true,
        "strictNullChecks": true,
        "useUnknownInCatchVariables": false,
        "allowJs": false,
        "skipLibCheck": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "rootDir": ".",
        "baseUrl": "src",
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": ["next-env.d.ts", ".next/types/**/*.ts", ".eslintrc.cjs", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
}
